import java.awt.Color;import java.awt.Graphics;import java.awt.image.BufferedImage;import java.io.IOException;import java.net.URL;import javax.imageio.ImageIO;import javax.swing.JApplet; public class img extends JApplet {   public void paint(Graphics p){	  BufferedImage image = null;	  try {		    URL url = new URL(getCodeBase(), "Koala.jpg");		    image = ImageIO.read(url);		} catch (IOException e) {		}	int w = image.getWidth()/4;    int h = image.getHeight()/4;    setSize(w,h);    for (int i = 0; i < h; i++) {      for (int j = 0; j < w; j++) {        int pixel = image.getRGB(j*4, i*4);        int alpha = (pixel >> 24) & 0xff;        int red = (pixel >> 16) & 0xff;        int green = (pixel >> 8) & 0xff;        int blue = (pixel) & 0xff;        p.setColor(new Color(round(red),round(green),round(blue),alpha));        p.fillRect(j, i, 1, 1);      }    }  }    public int round(int num){	  double n = num/100;	  num = (int)n;	  num*=100;	  return num;  }  }