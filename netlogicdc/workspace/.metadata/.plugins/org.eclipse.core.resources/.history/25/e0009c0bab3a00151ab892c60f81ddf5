import java.awt.Color;import java.awt.Graphics;import java.awt.image.BufferedImage;import java.io.IOException;import java.net.URL;import javax.imageio.ImageIO;import javax.swing.JApplet;public class img extends JApplet {	int simples[][] = {{255,0,0},{255,128,0},			{255,255,0},{128,255,0},{0,255,0},			{0,255,128},{0,255,255},{0,128,255},			{0,0,255},{127,0,255},{255,0,255},			{255,0,127},{128,128,128}};						/*			{51,0,0},{51,51,0},{25,51,0},			{51,25,0},{0,51,0},{0,51,25},			{51,25,0},{0,0,51},{25,0,51},			{51,0,51},{51,0,25},{0,0,0},			{102,0,0},{102,51,0},{102,102,0},			{51,102,0},{0,102,0},{0,102,51},			{0,102,102},{0,51,102},{0,0,102},			{51,0,102},{102,0,102},{102,0,51},			{32,32,32},{64,64,64},{96,96,96},			{128,128,128},{153,0,0},{153,76,0},			{153,153,0},{76,153,0},{0,153,0},			{0,153,76},{0,153,153},{0,76,153},			{0,0,153},{76,0,153},{153,0,153},			{153,0,76},{204,0,102},{204,0,204},			{102,0,204},{0,0,204},{0,102,204},			{0,204,204},{0,204,102},{0,204,0},			{102,204,0},{204,204,0},{204,102,0},			{204,0,0},{255,0,0},{255,128,0},			{255,255,0},{128,255,0},{0,255,0},			{0,255,128},{0,255,255},{0,128,255},			{0,0,255},{127,0,255},{255,0,255},			{255,0,127},{255,51,51},{255,153,51},			{255,255,51},{153,255,51},{51,255,51},			{51,255,153},{51,255,255},{51,153,255},			{51,51,255},{153,51,255},{255,51,255},			{255,51,153},{160,160,160},{192,192,192},			{255,102,178},{255,102,255},{178,102,255},			{102,102,255},{102,178,255},{102,255,255},			{102,255,178},{102,255,102},{178,255,102},			{255,255,102},{255,178,102},{255,102,102},			{255,153,153},{204,255,153},{153,255,255},			{255,204,153},{153,255,153},{153,204,255},			{255,255,153},{153,255,204},{153,153,255},			{255,204,204},{229,255,204},{204,255,255},			{255,229,204},{204,255,204},{204,229,255},			{255,255,204},{204,255,229},{204,204,255},			{204,153,255},{229,204,255},{224,224,224},			{255,153,255},{255,204,255},{255,255,255},			{255,153,204},{255,204,229}};			*/	int[][][] rgbs;			public void paint(Graphics p) {		BufferedImage image = null;		try {			URL url = new URL(getCodeBase(), "Hydrangeas.jpg");			image = ImageIO.read(url);		} catch (IOException e) {		}		int w = image.getWidth();		int h = image.getHeight();		rgbs = new int[3][w][h];		for (int i = 0; i < h; i++) {			for (int j = 0; j < w; j++) {				int pixel = image.getRGB(j, i);				int red = (pixel >> 16) & 0xff;				int green = (pixel >> 8) & 0xff;				int blue = (pixel) & 0xff;				p.setColor(round(new Color(red, green, blue)));				p.fillRect(j, i, 1, 1);			}		}	}	public Color round(Color c) {		int index = 0;		double d = 255;		for (int i = 0; i < simples.length; i++) {			int r = Math.abs(c.getRed()-simples[i][0]);			int g = Math.abs(c.getGreen()-simples[i][1]);			int b = Math.abs(c.getBlue()-simples[i][2]);			double d2 = (r+g+b)/3;			if (d2 < d) {				index = i;				d = d2;			}		}		return new Color(simples[index][0],simples[index][1],simples[index][2]);	}}