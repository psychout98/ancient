import java.awt.Color;import java.awt.Graphics;import java.awt.image.BufferedImage;import java.io.IOException;import java.net.URL;import javax.imageio.ImageIO;import javax.swing.JApplet;public class img extends JApplet {	Color simples[] = {new Color(51,0,0),new Color(51,51,0),new Color(25,51,0),			new Color(51,25,0),new Color(0,51,0),new Color(0,51,25),			new Color(51,25,0),new Color(0,0,51),new Color(25,0,51),			new Color(51,0,51),new Color(51,0,25),new Color(0,0,0),			new Color(102,0,0),new Color(102,51,0),new Color(102,102,0),			new Color(51,102,0),new Color(0,102,0),new Color(0,102,51),			new Color(0,102,102),new Color(0,51,102),new Color(0,0,102),			new Color(51,0,102),new Color(102,0,102),new Color(102,0,51),			new Color(32,32,32),new Color(64,64,64),new Color(96,96,96),			new Color(128,128,128),new Color(153,0,0),new Color(153,76,0),			new Color(153,153,0),new Color(76,153,0),new Color(0,153,0),			new Color(0,153,76),new Color(0,153,153),new Color(0,76,153),			new Color(0,0,153),new Color(76,0,153),new Color(153,0,153),			new Color(153,0,76),new Color(204,0,102),new Color(204,0,204),			new Color(102,0,204),new Color(0,0,204),new Color(0,102,204),			new Color(0,204,204),new Color(0,204,102),new Color(0,204,0),			new Color(102,204,0),new Color(204,204,0),new Color(204,102,0),			new Color(204,0,0),new Color(255,0,0),new Color(255,128,0),			new Color(255,255,0),new Color(128,255,0),new Color(0,255,0),			new Color(0,255,128),new Color(0,255,255),new Color(0,128,255),			new Color(0,0,255),new Color(127,0,255),new Color(255,0,255),			new Color(255,0,127),new Color(255,51,51),new Color(255,153,51),			new Color(255,255,51),new Color(153,255,51),new Color(51,255,51),			new Color(51,255,153),new Color(51,255,255),new Color(51,153,255),			new Color(51,51,255),new Color(153,51,255),new Color(255,51,255),			new Color(255,51,153),new Color(160,160,160),new Color(192,192,192),			new Color(255,102,178),new Color(255,102,255),new Color(178,102,255),			new Color(102,102,255),new Color(102,178,255),new Color(102,255,255),			new Color(102,255,178),new Color(102,255,102),new Color(178,255,102),			new Color(255,255,102),new Color(255,178,102),new Color(255,102,102),			new Color(255,153,153),new Color(204,255,153),new Color(153,255,255),			new Color(255,204,153),new Color(153,255,153),new Color(153,204,255),			new Color(255,255,153),new Color(153,255,204),new Color(153,153,255),			new Color(255,204,204),new Color(229,255,204),new Color(204,255,255),			new Color(255,229,204),new Color(204,255,204),new Color(204,229,255),			new Color(255,255,204),new Color(204,255,229),new Color(204,204,255),			new Color(204,153,255),new Color(229,204,255),new Color(224,224,224),			new Color(255,153,255),new Color(255,204,255),new Color(255,255,255),			new Color(255,153,204),new Color(255,204,229)};	int[][][] rgbs;			public void paint(Graphics p) {		BufferedImage image = null;		try {			URL url = new URL(getCodeBase(), "Hydrangeas.jpg");			image = ImageIO.read(url);		} catch (IOException e) {		}		int w = image.getWidth();		int h = image.getHeight();		rgbs = new int[3][w][h];		setSize(w, h);		for (int i = 0; i < h; i++) {			for (int j = 0; j < w; j++) {				int pixel = image.getRGB(j, i);				int red = (pixel >> 16) & 0xff;				int green = (pixel >> 8) & 0xff;				int blue = (pixel) & 0xff;				p.setColor(round(new Color(red, green, blue)));				p.fillRect(j, i, 1, 1);			}		}	}	public Color round(Color c) {		int index = 0;		int rd = 255, gd = 255, bd = 255;		for (int i = 0; i < simples.length; i++) {			int r = Math.abs(simples[i].getRed() - c.getRed());			int g = Math.abs(simples[i].getGreen() - c.getGreen());			int b = Math.abs(simples[i].getBlue() - c.getBlue());			if (r < rd && g < gd && b < bd) {				index = i;				rd = r;				gd = g;				bd = b;			}		}		return simples[index];	}}