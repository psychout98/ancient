			r.keyPress(KeyEvent.VK_TAB);
			Thread.sleep(8);
			r.keyRelease(KeyEvent.VK_TAB);
			Thread.sleep(8);
			if (a == 2) {
				r.keyPress(KeyEvent.VK_TAB);
				Thread.sleep(8);
				r.keyRelease(KeyEvent.VK_TAB);
				Thread.sleep(8);
			}
		}
		for(int i=0;i<26;i++){
			r.keyPress(KeyEvent.VK_TAB);
			Thread.sleep(8);
			r.keyRelease(KeyEvent.VK_TAB);
			Thread.sleep(8);
		}
		r.keyPress(KeyEvent.VK_SPACE);
		Thread.sleep(8);
		r.keyRelease(KeyEvent.VK_SPACE);
		Thread.sleep(3000);
		r.keyPress(KeyEvent.VK_T);
		Thread.sleep(8);
		r.keyRelease(KeyEvent.VK_T);
