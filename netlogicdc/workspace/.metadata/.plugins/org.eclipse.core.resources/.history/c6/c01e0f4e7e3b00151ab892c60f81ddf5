			Thread.sleep(8);
			r.keyRelease(KeyEvent.VK_TAB);
			Thread.sleep(8);
			r.keyPress(KeyEvent.VK_SPACE);
			Thread.sleep(8);
			r.keyRelease(KeyEvent.VK_SPACE);
			Thread.sleep(8);
		}
		r.keyRelease(KeyEvent.VK_SHIFT);
		Thread.sleep(8);
