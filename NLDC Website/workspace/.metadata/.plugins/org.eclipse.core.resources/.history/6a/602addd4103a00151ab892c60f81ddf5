import java.awt.Color;import java.awt.Component;import java.awt.Graphics;import java.awt.image.BufferedImage;import java.io.IOException;import javax.imageio.ImageIO;import javax.swing.JApplet; public class img extends JApplet {   private void marchThroughImage(Graphics p) throws IOException {    BufferedImage image = ImageIO.read(this.getClass().getResource("Koala.jpg"));    int w = image.getWidth();    int h = image.getHeight();    System.out.println("Width, Height : "+w+", "+h);    setSize(w,h);    for (int i = 0; i < h; i++) {      for (int j = 0; j < w; j++) {        int pixel = image.getRGB(j, i);        int alpha = (pixel >> 24) & 0xff;        int red = (pixel >> 16) & 0xff;        int green = (pixel >> 8) & 0xff;        int blue = (pixel) & 0xff;        p.setColor(new Color(alpha,red,green,blue));        p.fillRect(i, j, 1, 1);      }    }  }  }